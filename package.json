{
  "name": "the_game",
  "version": "1.0.0",
  "description": "Pixel Champions",
  "main": "src/index.js",
  "scripts": {
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec babel-node-ts src/index.js",
    "check-types": "tsc",
    "debug": "babel-node --inspect src/index.js",
    "complete": "eslint ./src && mocha --exit && node src/index.js",
    "pretest": "eslint ./src && mocha --exit",
    "posttest": "mocha --exit && eslint ./src",
    "test": "mocha --exit",
    "mocha": "mocha -w --require @babel/register",
    "mocha:perf": "node --allow-natives-syntax ./node_modules/mocha/bin/_mocha --opt ./test/mocha.opts ./test/performance.test.js",
    "lint": "eslint ./src",
    "lintfix": "eslint --fix ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/farefray/thegame.git"
  },
  "author": "Maksym Fedan&&Jacek Kizinski",
  "license": "ISC",
  "dependencies": {
    "babel-node-ts": "^1.0.3",
    "benchtest": "^2.0.2",
    "cors": "^2.8.5",
    "easystarjs": "^0.4.3",
    "eslint": "6.1.0",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "global": "^4.4.0",
    "http": "0.0.0",
    "keyv": "^3.1.0",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "nodemon": "^1.19.1",
    "npm": "^6.9.0",
    "prettier": "^1.18.2",
    "rewire": "^4.0.1",
    "socket.io": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.7.0",
    "@babel/register": "^7.6.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "should": "^13.2.3",
    "typescript": "^3.7.2"
  }
}
